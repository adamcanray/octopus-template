{
  "name": "octopus-one",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-slick": "^0.26.1"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.tailwind.css -o src/styles/index.css",
    "watch:css": "postcss src/styles/index.tailwind.css -o src/styles/index.css -w",
    "start": "concurrently \"npm run watch:css\" \"react-scripts start\"",
    "build": "concurrently \"npm run build:css\" \"&& react-scripts build\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "autoprefixer": "^9.7.6",
    "concurrently": "^5.2.0",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1",
    "react-emotion": "^10.0.0",
    "react-player": "^2.0.1",
    "react-rating": "^2.0.5",
    "react-router-dom": "^5.2.0",
    "react-truncate": "^2.4.0",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^1.4.6"
  }
}
